{
    "builds": [{
        "src": "Django_Application/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9.13" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "Django_Application/wsgi.py"
        }
    ],
    "rewrites": [
        {"source": "/(.*)", "destination": "/"}
    ]
}
